{
  "name": "Facebook Quick Share",
   "manifest_version": 2,
  "version": "1.9",
  "description":
    "Facebook Quick Share will allow you to quickly share/like any site to your FB wall. Great for sites without Facebook integration.",
  "icons": {"16": "f.png", "48": "f.png", "128": "f.png"},
  "permissions": ["tabs", "contextMenus", "notifications","https://*/*", "http://*/*"],
  "content_security_policy": "script-src 'self' https://connect.facebook.net  https://www.facebook.com; object-src 'self' https://connect.facebook.net",
   "background": {
    "page": "background.html"
  },
   "browser_action": {
    "default_icon": "f.png", 
    "default_title": "Facebook Quick Share",
    "default_popup": "options.html"
  }
}